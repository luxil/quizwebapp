doctype html
html(lang='en')
    head
        meta(charset='utf-8')
        meta(http-equiv='X-UA-Compatible', content='IE=edge')
        meta(name='viewport', content='width=device-width, initial-scale=1')
        title Scores
        link(rel='icon', type='image/png', href='images/favicon-32x32.png', sizes='32x32')
        link(rel='icon', type='image/png', href='images/favicon-16x16.png', sizes='16x16')
        link(rel='stylesheet', href='css/success.css')
        // Bootstrap
        link(href='css/bootstrap.min.css', rel='stylesheet')
        script(src='/socket.io/socket.io.js')
        script(src='https://cdn.socket.io/socket.io-1.2.0.js')
        script(src='js/success.js')
    body(onload='score()')
        #logo
            img(src='images/logo6.gif')
            p#id1 ID:
                el#id #{nummer}
        .jumbotron(style='text-align: center; height: 200px;')
            h2  Die Bestenliste von Quiz ID
                el#id  #{nummer}
        ul#liste.list-group
        form#formReset(action='/',method='post')
            input#hidden(name='hidden',type="hidden",value='',form="formReset")
        script(src='https://ajax.googleapis.com/ajax/libs/jquery/1.11.3/jquery.min.js')
        script(src='js/bootstrap.min.js')
